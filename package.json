{
  "name": "mcp-apple-simulator",
  "version": "2.1.0",
  "description": "MCP server for building, running, and testing Apple platform projects (iOS, macOS, tvOS, watchOS, visionOS)",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest src/__tests__/unit/",
    "test:integration": "jest src/__tests__/integration/"
  },
  "keywords": [
    "mcp",
    "apple",
    "ios",
    "macos",
    "tvos",
    "watchos",
    "visionos",
    "simulator",
    "xcode",
    "spm",
    "swift"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^5.3.3"
  }
}
