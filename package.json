{
  "name": "mcp-xcode-server",
  "version": "2.4.0",
  "description": "MCP server for Xcode - build, test, run, and manage Apple platform projects (iOS, macOS, tvOS, watchOS, visionOS)",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-xcode-server": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest src/__tests__/unit/",
    "test:integration": "jest src/__tests__/integration/",
    "postinstall": "cd XcodeProjectModifier && swift build -c release",
    "build:modifier": "cd XcodeProjectModifier && swift build -c release"
  },
  "keywords": [
    "mcp",
    "apple",
    "ios",
    "macos",
    "tvos",
    "watchos",
    "visionos",
    "simulator",
    "xcode",
    "spm",
    "swift"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3",
    "commander": "^12.0.0",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^5.3.3"
  }
}
